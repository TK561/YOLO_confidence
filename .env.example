# YOLO Detection Settings
# Copy this file to .env and update with your values

# Output directory (default: Desktop)
OUTPUT_DIR=C:\Users\[username]\OneDrive\Desktop

# Detection settings
CONFIDENCE_THRESHOLD=0.25
IOU_THRESHOLD=0.45
DUPLICATE_IOU_THRESHOLD=0.8

# Display settings
FONT_SIZE=3.5
FONT_THICKNESS=6
BOX_THICKNESS=12

# Model settings
MODEL_NAME=yolov8n.pt

# Debug mode (true/false)
DEBUG_MODE=true